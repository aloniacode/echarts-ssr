  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.2",
    "@types/node": "^24.0.14",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@hono/node-server": "^1.16.0",
    "@napi-rs/canvas": "^0.1.44",
    "echarts": "^5.6.0",
    "hono": "^4.8.5"
  }
}
