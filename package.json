{
  "name": "echarts-ssr",
  "version": "1.0.0",
  "description": "The server-side rendering of echarts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "swc src -d dist -D --strip-leading-paths"
